{
  "name": "pgdesign-backend",
  "version": "1.0.0",
  "description": "Backend API for PG Design website",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed:make": "knex seed:make",
    "upload:images": "node scripts/uploadImages.js",
    "public:urls": "node scripts/updateToPublicUrls.js",
    "seed:project-page": "node scripts/seedProjectPageFromMock.js",
    "seed:subcategories": "node scripts/seedProjectSubCategoriesFromMock.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-slow-down": "^1.6.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "knex": "^2.5.1",
    "minio": "^7.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.14",
    "sharp": "^0.32.4",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-slow-down": "^1.3.5",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "minio",
    "api",
    "pgdesign"
  ],
  "author": "PG Design Team",
  "license": "MIT"
}
